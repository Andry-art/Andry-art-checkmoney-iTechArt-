{
  "env": {
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "project": "./tsconfig.json"
  },
  "root": true,
  "plugins": [
    "@typescript-eslint",
    "eslint-plugin-jest"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error"
    ]
  }
}
